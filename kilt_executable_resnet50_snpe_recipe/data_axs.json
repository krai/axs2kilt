{
    "_parent_entries": [ [ "^", "byname", "shell" ] ],
    "_producer_rules": [
        [ [ "compiled", "kilt_executable", "resnet50", "device=snpe" ], [["run"]], { "return_saved_record_entry": false }, ["shell_cmd_with_subs"] ]
    ],

    "lib_kilt_path": [ "^", "execute", [[
        [ "byquery", "compiled,lib_kilt" ],
        [ "get_path" ]
    ]] ],

    "loadgen_path": [ "^", "execute", [[
        [ "byquery", "git_repo,collection,repo_name=axs2mlperf" ],
        [],
        [ "byquery", "compiled,loadgen_c" ],
        [ "get_path" ]
    ]] ],

    "protobuf_path": [ "^", "execute", [[
        [ "byquery", "compiled,protobuf" ],
        [ "get_path" ]
    ]] ],

    "snpe_path": [ "^", "execute", [[
        [ "byquery", "extracted,snpe_lib" ],
        [ "get_path" ]
    ]] ],

    "cjson_lib_path": [ "^", "execute", [[
        [ "byquery", "lib,lib_name=cjson" ],
        [ "get_path", ["cJSON.o"] ]
     ]] ],
    "sut_name": ["^", "func", "socket.gethostname" ],

    "snpe_lib_sut_insert": [ "^^", "case",   [ ["^^", "get", "sut_name"],
        "eb6", "aarch64-ubuntu-gcc7.5"],
        { "default_value": "x86_64-linux-clang"}
    ],

    "protobuf_sut_lib": [ "^^", "case",   [ ["^^", "get", "sut_name"],
        "dyson", "lib64"],
        { "default_value": "lib"}
    ],

    "return_this_entry": [ "^^", "execute", [[
        [ "get_kernel" ],
        [ "work_collection" ],
        [ "attached_entry", [ "compiled_kilt_executable_resnet50_snpe", {
                "tags": [ "compiled", "kilt_executable", "resnet50" ],
                "device" : "snpe",
                "file_name" : "qaic_image_classifier"
            } ] ],
        [ "save" ]
    ]] ],

    "in_dir": [ "^^", "execute", [[
        [ "get", "return_this_entry" ],
        [ "get_path" ],
        0,
        [ "func", "os.path.dirname" ]
    ]] ],

    "compiler_deps": "g++ -c -I../ -DCK_HOST_OS_NAME2_LINUX=1 -DCK_HOST_OS_NAME_LINUX=1 -DCK_TARGET_OS_NAME2_LINUX=1 -DCK_TARGET_OS_NAME_LINUX=1 -DKILT_BENCHMARK_STANDALONE_CLASSIFICATION -DKILT_DEVICE_SNPE -DKILT_CONFIG_FROM_JSON -DKILT_CONFIG_TRANSLATE_X -std=c++11 -O3 -ffast-math -DR282 -Wall -Wno-sign-compare -Wno-unused-result -std=c++17 -I/install/ -I#{cjson_lib_entry.include_path}# -I#{lib_kilt_path}#/cJSON/ -I#{lib_kilt_path}#/include -I#{loadgen_path}#/include -I#{protobuf_path}#/install/include -I#{snpe_path}#/include/zdl",
    "harness_o":[ "^^", "substitute", "#{compiler_deps}#  #{lib_kilt_path}#/include/benchmarks/harness/harness.cpp -o harness.o"],

    "shell_cmd_with_subs": [ "^^", "substitute", "#{harness_o}# && g++ harness.o #{cjson_lib_path}# -o qaic_image_classifier -L#{loadgen_path}#/lib -lmlperf_loadgen -L#{protobuf_path}#/install/#{protobuf_sut_lib}# -lprotobuf -L#{lib_kilt_path}#/lib -L#{snpe_path}#/lib/#{snpe_lib_sut_insert}# -ldl -lSNPE -lpthread"]
}

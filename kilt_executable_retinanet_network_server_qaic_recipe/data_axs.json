{
    "_parent_entries": [ [ "^", "byname", "shell" ] ],
    "_producer_rules": [
        [ [ "compiled", "kilt_executable", "device=qaic", "retinanet_server" ], [["run"]], { "return_saved_record_entry": false }, ["shell_cmd_with_subs"] ]
    ],

    "device": "qaic",

    "lib_kilt_path": [ "^", "execute", [[
        [ "byquery", "compiled,lib_kilt" ],
        [ "get_path" ]
    ]] ],

    "loadgen_path": [ "^", "execute", [[
	[ "byquery", "git_repo,collection,repo_name=axs2mlperf" ],
	[],
        [ "byquery", "compiled,loadgen_c" ],
        [ "get_path" ]
    ]] ],

    "protobuf_path": [ "^", "execute", [[
	[ "byquery", "compiled,protobuf" ],
        [ "get_path" ]
    ]] ],

    "cjson_lib_path": [ "^", "execute", [[
        [ "byquery", "lib,lib_name=cjson" ],
        [ "get_path", ["cJSON.o"] ]
     ]] ],
    "sut_name": ["^", "func", "socket.gethostname" ],

    "protobuf_sut_lib": [ "^^", "case",   [ ["^^", "get", "sut_name"],
        "dyson", "lib64"],
        { "default_value": "lib"}
    ],
    "return_this_entry": [ "^^", "execute", [[
        [ "get_kernel" ],
        [ "work_collection" ],
        [ "attached_entry", [ "compiled_kilt_executable_retinanet_network_server_qaic", {
                "tags": [ "compiled", "kilt_executable", "retinanet_server" ],
                "file_name" : "qaic_object_detection_server",
                "device": [ "^^", "substitute", "#{device}#" ]
            } ] ],
        [ "save" ]
    ]] ],

    "in_dir": [ "^^", "execute", [[
        [ "get", "return_this_entry" ],
        [ "get_path" ],
        0,
        [ "func", "os.path.dirname" ]
    ]] ],

    "compiler_deps": "g++ -c -I../ -DCK_HOST_OS_NAME2_LINUX=1 -DCK_HOST_OS_NAME_LINUX=1 -DCK_TARGET_OS_NAME2_LINUX=1 -DCK_TARGET_OS_NAME_LINUX=1 -DKILT_BENCHMARK_NETWORK_OBJECT_DETECTION_SERVER -DKILT_DEVICE_QAIC -DKILT_CONFIG_FROM_JSON -DKILT_CONFIG_TRANSLATE_X -std=c++17 -O3 -ffast-math -march=znver2  -DENABLE_ZEN2 -DR282 -DMODEL_RX50 -Wall -Wno-sign-compare  -std=c++17 -I/opt/qti-aic/dev/inc/ -I/opt/qti-aic/dev/inc/qaic-api-common -I/install/ -I#{cjson_lib_entry.include_path}# -I#{lib_kilt_path}#/cJSON/ -I#{lib_kilt_path}#/include -I#{loadgen_path}#/include -I#{protobuf_path}#/install/include",
    "server_o":[ "^^", "substitute", "#{compiler_deps}#  #{lib_kilt_path}#/include/benchmarks/network/object-detection/server/server.cpp -o server.o"],
    "qaicinfapi_o":[ "^^", "substitute", "#{compiler_deps}# #{lib_kilt_path}#/include/devices/qaic/api/master/QAicInfApi.cpp  -o QAicInfApi.o"],

    "shell_cmd_with_subs": [ "^^", "substitute", "#{server_o}# && #{qaicinfapi_o}# && g++ server.o QAicInfApi.o #{cjson_lib_path}# -o qaic_object_detection_server -L#{loadgen_path}#/lib -lmlperf_loadgen -L#{lib_kilt_path}#/lib -lQAic -lpthread"]

}
